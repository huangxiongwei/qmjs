.userinfo-container{
  width: 100%;
  height: 100rpx;
  position:absolute;
}
.userinfo {
  display: flex;
  flex-direction: row;
  align-items:center;
  justify-content: flex-start;
  border-bottom: 1px solid #aaa;
}

.userinfo-avatar {
  width: 80rpx;
  height: 80rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.cardList{
  width: 20px;
  height: 20px;
  margin-right: 5px;
}
